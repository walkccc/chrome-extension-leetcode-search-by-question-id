{
  "manifest_version": 3,
  "name": "LeetCode Search by Question ID",
  "version": "0.0.0.4",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "host_permissions": ["*://*/*"],
  "permissions": ["storage"],
  "action": {
    "default_icon": "icons/48.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "serviceWorker.js",
    "type": "module"
  }
}
